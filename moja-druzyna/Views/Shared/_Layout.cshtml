<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>mojadrużyna - @ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/views/shared/_Layout.css" asp-append-version="true"/>
</head>
<body>
    <div class="screen">
        <header>
            <nav class="upper-panel navbar-expand-sm">
                <img src="~/icons/layout/split-bar.png" style="position:absolute;width:2px;height:49px;left:256px;top:8px;" />
                <div style="position:absolute;right:0px;top:8px;">
                    <partial name="_LoginPartial" />
                </div>
            </nav>
        </header>
        <div class="center-section-container">
            <input type="checkbox" id="is-sb-collapsed" />
            <label id="hamburger" for="is-sb-collapsed">
                <img id="hamburger-icon" src="~/icons/navigation/nav-hamburger.svg" />
            </label>
            <div class="left-panel">
            </div>
            <div class="sidebar">
                <div id="sidebar-header">
                </div>
                <div id="sidebar-content">
                    <div class="navigations">
                        <div id="sidebar-profile">
                            <div id="sidebar-section-header">
                                <img src="~/icons/main/person.png" style="width:24px;height:24px;margin-left:8px;margin-top:4px;" />
                                <div style="margin:4px;">
                                    <a class="text-dark" asp-controller="profile" asp-action="personaldata">
                                        Profil
                                    </a>
                                </div>
                            </div>
                            <div style="margin-left:16px;margin-top:8px;">
                                <a class="text-dark" asp-controller="profile" asp-action="personaldata">
                                    Dane
                                </a>
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                <a class="text-dark" asp-controller="profile" asp-action="servicehistory">
                                    Historia służby
                                </a>
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                <a class="text-dark" asp-controller="profile" asp-action="ranks">
                                    Stopnie
                                </a>
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                <a class="text-dark" asp-controller="profile" asp-action="achievments">
                                    Osiągnięcia
                                </a>
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                <a class="text-dark" asp-controller="profile" asp-action="roles">
                                    Role
                                </a>
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                <a class="text-dark" asp-controller="profile" asp-action="coursesandpermissions">
                                    Kursy szkoleniowe i uprawnienia
                                </a>
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                <a class="text-dark" asp-controller="profile" asp-action="gdprconsents">
                                    Zgody RODO
                                </a>
                            </div>
                        </div>
                        <div id="sidebar-reports">
                            <div id="sidebar-section-header">
                                <img src="~/icons/main/sticky-note.png" style="width:24px;height:24px;margin-left:8px;margin-top:4px;" />
                                <div style="margin:4px;">
                                    Raporty
                                </div>
                            </div>
                            <div style="margin-left:16px;margin-top:8px;">
                                1
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                2
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                3
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                4
                            </div>
                        </div>
                        <div id="sidebar-team">
                            <div id="sidebar-section-header">
                                <img src="~/icons/main/flag.png" style="width:24px;height:24px;margin-left:8px;margin-top:4px;" />
                                <div style="margin:4px;">
                                    Drużyna
                                </div>
                            </div>
                            <div style="margin-left:16px;margin-top:8px;">
                                Moja drużyna
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                2
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                3
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                4
                            </div>
                        </div>
                        <div id="sidebar-ranking">
                            <div id="sidebar-section-header">
                                <img src="~/icons/main/cup.png" style="width:24px;height:24px;margin-left:8px;margin-top:4px;" />
                                <div style="margin:4px;">
                                    Ranking
                                </div>
                            </div>
                            <div style="margin-left:16px;margin-top:8px;">
                                Punktacje
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                Zasady
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                3
                            </div>
                            <div style="margin-left:16px;margin-top:16px;">
                                4
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="container">
                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>